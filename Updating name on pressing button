import React,{ useState } from 'react';

import {
  Text,
  View,
  Button,
} from 'react-native';

const App = () => {
    const [name, setName]= useState("Samad");

    function testName(){
        setName("Sam")
    }
    return(
    <View>
        <Text style = {{fontSize : 20}}> {name} </Text>
        <Button title= 'Update Name' onPress= {testName} />
    </View>
    );
};

export default App;
